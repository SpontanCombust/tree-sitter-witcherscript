{
  "name": "tree-sitter-witcherscript",
  "version": "0.5.1",
  "description": "WitcherScript grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.ws",
      "file-types": [
        "ws"
      ],
      "injection-regex": "^(ws|witcherscript)$",
      "highlights": null
    }
  ]
}
